    "def analyze_text_features(df, text_col):\n",
    "    \"\"\"\n",
    "    Analyze basic statistics for text data.\n",
    "\n",
    "    Parameters:\n",
    "        df (pd.DataFrame): The dataset containing text data.\n",
    "        text_col (str): The name of the text column to analyze.\n",
    "\n",
    "    Returns:\n",
    "        None\n",
    "    \"\"\"\n",
    "    # Calculate basic text statistics\n",
    "    text_lengths = df[text_col].apply(lambda x: len(str(x).split()))\n",
    "    print(f\"Text Column '{text_col}' Analysis:\\n\")\n",
    "    print(f\"Average Number of Words: {text_lengths.mean():.2f}\")\n",
    "    print(f\"Maximum Number of Words: {text_lengths.max()}\")\n",
    "    print(f\"Minimum Number of Words: {text_lengths.min()}\")\n",
    "\n",
    "    # Visualize text lengths\n",
    "    plt.figure(figsize=(8, 4))\n",
    "    sns.histplot(text_lengths, bins=30, kde=True, color='green')\n",
    "    plt.title(f'Distribution of Text Lengths in {text_col}')\n",
    "    plt.xlabel('Number of Words')\n",
    "    plt.ylabel('Frequency')\n",
    "    plt.show()\n",
    "\n",
    "analyze_text_features(df, 'Review Text')"
   ]
