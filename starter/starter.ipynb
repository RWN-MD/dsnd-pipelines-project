   "source": [
    "import nltk\n",
    "from nltk.sentiment import SentimentIntensityAnalyzer\n",
    "from nltk.tokenize import word_tokenize\n",
    "from nltk.chunk import ne_chunk\n",
    "from nltk.tag import pos_tag\n",
    "from sklearn.feature_extraction.text import CountVectorizer\n",
    "\n",
    "nltk.download('averaged_perceptron_tagger')\n",
    "nltk.download('maxent_ne_chunker')\n",
    "nltk.download('words')\n",
    "nltk.download('punkt')\n",
    "nltk.download('vader_lexicon')\n",
    "\n",
    "def engineer_and_explore_features(df, target_col='Recommended IND'):\n",
    "    \"\"\"\n",
    "    Perform feature engineering and dataset exploration.\n",
    "\n",
    "    This function focuses on:\n",
    "    1. Removing low-quality, low-impact, or redundant features.\n",
    "    2. Creating new numerical features using NLP techniques:\n",
    "        - Sentiment scores.\n",
    "        - Word counts.\n",
    "        - Entity counts.\n",
    "    3. Adding text embeddings.\n",
    "    4. Preparing the dataset for feature exploration and scaling.\n",
    "\n",
    "    Parameters:\n",
    "        df (pd.DataFrame): Input dataset.\n",
    "        target_col (str): Name of the target column (default: 'Recommended IND').\n",
    "\n",
    "    Returns:\n",
    "        pd.DataFrame: Updated dataset with new features and cleaned data.\n",
    "    \"\"\"\n",
    "    \n",
    "    print(\"\\nStep 1: Removing Low-Impact/Redundant Features\")\n",
    "    df = df.drop(columns=['Clothing ID'], errors='ignore')\n",
    "    print(f\"Removed low-impact features.\")\n",
    "    df = df.drop(columns=['Title', 'Review Text'], errors='ignore')\n",
    "    print(f\"Removed redundant features.\")\n",
    "\n",
    "    print(\"\\nStep 2: Creating new features using NLP techniques\")\n",
    "    # Normalize and tokenize text\n",
    "    df['Processed Review'] = df['Full Review'].str.lower().apply(word_tokenize)\n",
    "    print(f'Tokenized Full Review to new column Processed Review.')\n",
    "    \n",
    "    # Sentiment Analysis\n",
    "    sentiment_analyzer = SentimentIntensityAnalyzer()\n",
    "    df['Sentiment Score'] = df['Full Review'].apply(\n",
    "        lambda x: sentiment_analyzer.polarity_scores(x)['compound']\n",
    "    )\n",
    "    print(f\"Added Sentiment Scores.\")\n",
    "    \n",
    "    # Word Count\n",
    "    df['Word Count'] = df['Processed Review'].apply(len)\n",
    "\n",
    "    # POS Tagging\n",
    "    pos_tags = [pos_tag(tokens) for tokens in df['Processed Review']]\n",
    "\n",
    "    # Entity Count: Number of named entities (proper nouns)\n",
    "    df['Entity Count'] = [sum(1 for _, tag in tags if tag in ('NNP', 'NNPS')) for tags in pos_tags]\n",
    "    \n",
    "    # POS Tagging\n",
    "    df['POS Tags'] = [pos_tag(tokens) for tokens in df['Processed Review']]\n",
    "    print(f\"Generated POS tags.\")\n",
    "\n",
    "    # Named Entity Recognition (NER)\n",
    "    df['NER Tags'] = [ne_chunk(pos_tag(tokens)) for tokens in df['Processed Review']]\n",
    "    print(f\"Extracted NER tags. {df.columns.tolist()}\")\n",
    "    \n",
    "    print(f\"Added Sentiment Score, Word Count, Entity Count features, POS Tags, and NER Tags.\")\n",
    "\n",
    "    print(\"\\nStep 3: Adding Text Embeddings\")\n",
    "    # Create text embeddings using CountVectorizer\n",
    "    embedding_vectorizer = CountVectorizer(max_features=500)\n",
    "    embeddings = embedding_vectorizer.fit_transform(df['Full Review']).toarray()\n",
    "    embedding_features = pd.DataFrame(embeddings, columns=embedding_vectorizer.get_feature_names_out())\n",
    "    df = pd.concat([df.reset_index(drop=True), embedding_features.reset_index(drop=True)], axis=1)\n",
    "    print(f\"Added {embedding_features.shape[1]} text embedding features.\")\n",
    "\n",
    "    print(\"\\nStep 4: Preparing for Numerical Feature Scaling\")\n",
    "    # Numerical Features\n",
    "    numerical_features = ['Age', 'Positive Feedback Count', 'Sentiment Score', 'Word Count', 'Entity Count']\n",
    "    print(\"Numerical Features for Exploration:\", numerical_features)\n",
    "    \n",
    "    print(\"\\nStep 5: Dataset Overview\")\n",
    "    # Summary of dataset\n",
    "    print(\"Dataset Shape:\", df.shape)\n",
    "    print(\"Feature Sample:\")\n",
    "    print(df.head())\n",
    "\n",
    "    # Split dataset into features and target\n",
    "    X = df.drop(columns=[target_col], errors='ignore')\n",
    "    y = df[target_col]\n",
    "    print(f\"Feature Set Shape: {X.shape}, Target Set Shape: {y.shape}\")\n",
    "\n",
    "    # Return dataset for exploration\n",
    "    return df\n",
    "\n",
    "df = engineer_and_explore_features(df)"
   ]
