   "source": [
    "from sklearn.feature_extraction.text import TfidfVectorizer\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "from sklearn.compose import ColumnTransformer\n",
    "from sklearn.pipeline import Pipeline\n",
    "from sklearn.ensemble import RandomForestClassifier\n",
    "from sklearn.model_selection import train_test_split\n",
    "import numpy as np\n",
    "\n",
    "def preprocess_and_train_pipeline(df):\n",
    "    \"\"\"\n",
    "    Preprocesses the data, builds a pipeline, and evaluates a Random Forest Classifier.\n",
    "\n",
    "    Steps:\n",
    "    1. Combines Title and Review Text into a single column ('Full Review').\n",
    "    2. Drops original text columns ('Title' and 'Review Text').\n",
    "    3. Applies log transformation and standardization to numerical features.\n",
    "    4. Performs one-hot encoding on categorical columns.\n",
    "    5. Splits the data into training and test sets.\n",
    "    6. Defines a preprocessing pipeline with TF-IDF for text, passthrough for numerical and categorical features.\n",
    "    7. Trains a Random Forest Classifier with class weights for imbalance.\n",
    "    8. Evaluates the pipeline on training and test data.\n",
    "\n",
    "    Parameters:\n",
    "        df (pd.DataFrame): Input DataFrame with raw data.\n",
    "\n",
    "    Returns:\n",
    "        tuple: The trained pipeline, training accuracy, test accuracy, X_train, y_train, preprocessor, X_test, y_test, numerical_cols, and remaining_cols.\n",
    "    \"\"\"\n",
    "    # Combine Title and Review Text into a single column\n",
    "    df['Full Review'] = df['Title'] + \" \" + df['Review Text']\n",
    "\n",
    "    # Drop original text columns\n",
    "    df = df.drop(columns=['Title', 'Review Text'])\n",
    "\n",
    "    # Prepare numerical columns\n",
    "    numerical_cols = ['Age', 'Positive Feedback Count']\n",
    "\n",
    "    # Apply log transformation to handle skewness in Positive Feedback Count\n",
    "    df['Positive Feedback Count'] = np.log1p(df['Positive Feedback Count'])\n",
    "\n",
    "    # Standardize numerical columns\n",
    "    scaler = StandardScaler()\n",
    "    df[numerical_cols] = scaler.fit_transform(df[numerical_cols])\n",
    "\n",
    "    # Categorical columns for one-hot encoding\n",
    "    categorical_cols = ['Clothing ID', 'Division Name', 'Department Name', 'Class Name']\n",
    "\n",
    "    # Perform one-hot encoding on categorical columns\n",
    "    df = pd.get_dummies(df, columns=categorical_cols, drop_first=True)\n",
    "\n",
    "    # Print processed data shape\n",
    "    print(\"Processed Data Shape:\", df.shape)\n",
    "\n",
    "    # Define feature columns\n",
    "    text_column = 'Full Review'\n",
    "    remaining_cols = list(df.columns.drop(['Recommended IND', 'Full Review']))\n",
    "    target = 'Recommended IND'\n",
    "\n",
    "    # Split data into features and target\n",
    "    X = df.drop(columns=[target])\n",
    "    y = df[target]\n",
    "    X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.1, random_state=27)\n",
    "\n",
    "    # Define preprocessing steps\n",
    "    preprocessor = ColumnTransformer(\n",
    "        transformers=[\n",
    "            ('tfidf', TfidfVectorizer(max_features=500), text_column),\n",
    "            ('num', 'passthrough', numerical_cols),\n",
    "            ('cat', 'passthrough', remaining_cols)\n",
    "        ]\n",
    "    )\n",
    "\n",
    "    # Define pipeline with class_weight for imbalance\n",
    "    pipeline = Pipeline(steps=[\n",
    "        ('preprocessor', preprocessor),\n",
    "        ('classifier', RandomForestClassifier(random_state=44, class_weight='balanced'))\n",
    "    ])\n",
    "\n",
    "    # Fit the pipeline\n",
    "    pipeline.fit(X_train, y_train)\n",
    "\n",
    "    # Evaluate the pipeline\n",
    "    train_accuracy = pipeline.score(X_train, y_train)\n",
    "    test_accuracy = pipeline.score(X_test, y_test)\n",
    "\n",
    "    print(f\"Training Accuracy: {train_accuracy:.2f}\")\n",
    "    print(f\"Testing Accuracy: {test_accuracy:.2f}\")\n",
    "\n",
    "    return pipeline, train_accuracy, test_accuracy, X_train, y_train, preprocessor, X_test, y_test, numerical_cols, remaining_cols, df\n",
    "\n",
    "pipeline, train_accuracy, test_accuracy, X_train, y_train, preprocessor, X_test, y_test, numerical_cols, remaining_cols, df = preprocess_and_train_pipeline(df)"
   ]
