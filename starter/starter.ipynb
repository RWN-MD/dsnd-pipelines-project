    "from sklearn.utils.class_weight import compute_class_weight\n",
    "from sklearn.ensemble import GradientBoostingClassifier, AdaBoostClassifier\n",
    "from sklearn.linear_model import LogisticRegression\n",
    "from sklearn.utils import resample\n",
    "from sklearn.pipeline import Pipeline\n",
    "from sklearn.metrics import classification_report, confusion_matrix\n",
    "from sklearn.ensemble import RandomForestClassifier\n",
    "from sklearn.model_selection import GridSearchCV\n",
    "import joblib\n",
    "\n",
    "\n",
    "def resample_and_evaluate(X_train, y_train, X_test, y_test, preprocessor, numerical_cols, remaining_cols):\n",
    "    \"\"\"\n",
    "    Performs random oversampling, hyperparameter tuning, and evaluation with multiple classifiers.\n",
    "\n",
    "    Steps:\n",
    "    1. Applies random oversampling to balance the dataset.\n",
    "    2. Defines a RandomForest pipeline and performs GridSearchCV for hyperparameter tuning.\n",
    "    3. Evaluates the optimized RandomForest model and performs feature importance analysis.\n",
    "    4. Tests alternative models (Gradient Boosting, AdaBoost, Logistic Regression) and evaluates their performance.\n",
    "    5. Saves the optimized RandomForest model to a file.\n",
    "\n",
    "    Parameters:\n",
    "        X_train (pd.DataFrame): Training features.\n",
    "        y_train (pd.Series): Training target variable.\n",
    "        X_test (pd.DataFrame): Test features.\n",
    "        y_test (pd.Series): Test target variable.\n",
    "        preprocessor (ColumnTransformer): Preprocessing steps for the pipeline.\n",
    "\n",
    "    Returns:\n",
    "        dict: A dictionary containing evaluation metrics for each model.\n",
    "    \"\"\"\n",
    "    # Step 1: Compute custom class weights\n",
    "    class_weights = compute_class_weight('balanced', classes=np.array([0, 1]), y=y_train)\n",
    "    class_weight_dict = {0: class_weights[0] * 1.5, 1: class_weights[1]}\n",
    "    print(\"Custom Class Weights:\", class_weight_dict)\n",
    "\n",
    "    # Apply random oversampling\n",
    "    X_train_minority = X_train[y_train == 0]\n",
    "    X_train_majority = X_train[y_train == 1]\n",
    "    X_train_minority_oversampled = resample(\n",
    "        X_train_minority,\n",
    "        replace=True,\n",
    "        n_samples=len(X_train_majority),\n",
    "        random_state=44\n",
    "    )\n",
    "    X_train_resampled = pd.concat([X_train_majority, X_train_minority_oversampled])\n",
    "    y_train_resampled = pd.concat([y_train[y_train == 1], y_train[y_train == 0].sample(len(X_train_majority), replace=True, random_state=44)])\n",
    "\n",
    "    # Step 2: Define the pipeline\n",
    "    pipeline = Pipeline(steps=[\n",
    "        ('preprocessor', preprocessor),\n",
    "        ('classifier', RandomForestClassifier(random_state=44, class_weight=class_weight_dict))\n",
    "    ])\n",
    "\n",
    "    # Define the grid search parameters\n",
    "    param_grid = {\n",
    "        'classifier__n_estimators': [100, 200, 300],\n",
    "        'classifier__max_depth': [10, 15, 20],\n",
    "        'classifier__min_samples_split': [10, 15, 20],\n",
    "        'classifier__min_samples_leaf': [5, 10]\n",
    "    }\n",
    "\n",
    "    # Perform GridSearchCV\n",
    "    grid_search = GridSearchCV(\n",
    "        estimator=pipeline,\n",
    "        param_grid=param_grid,\n",
    "        scoring='f1',\n",
    "        cv=5,\n",
    "        n_jobs=-1,\n",
    "        verbose=2\n",
    "    )\n",
    "    grid_search.fit(X_train_resampled, y_train_resampled)\n",
    "    \n",
    "    # Best parameters from grid search\n",
    "    best_params = grid_search.best_params_\n",
    "    print(\"Best Parameters:\", best_params)\n",
    "    print(\"Best F1-Score from Cross-Validation:\", grid_search.best_score_)\n",
    "\n",
    "    # Optimized pipeline\n",
    "    optimized_pipeline = grid_search.best_estimator_\n",
    "\n",
    "    # Evaluate the optimized model\n",
    "    train_accuracy = optimized_pipeline.score(X_train, y_train)\n",
    "    test_accuracy = optimized_pipeline.score(X_test, y_test)\n",
    "    print(f\"Optimized Training Accuracy: {train_accuracy:.2f}\")\n",
    "    print(f\"Optimized Testing Accuracy: {test_accuracy:.2f}\")\n",
    "\n",
    "    y_pred = optimized_pipeline.predict(X_test)\n",
    "    print(\"Classification Report:\\n\", classification_report(y_test, y_pred))\n",
    "    print(\"Confusion Matrix:\\n\", confusion_matrix(y_test, y_pred))\n",
    "\n",
    "    # Feature importance analysis (optional for RandomForestClassifier)\n",
    "    if isinstance(optimized_pipeline.named_steps['classifier'], RandomForestClassifier):\n",
    "        feature_importances = optimized_pipeline.named_steps['classifier'].feature_importances_\n",
    "        feature_names = numerical_cols + remaining_cols + [\"Full Review\"]\n",
    "        tfidf_feature_names = optimized_pipeline.named_steps['preprocessor'].transformers_[0][1].get_feature_names_out()\n",
    "        all_feature_names = list(tfidf_feature_names) + numerical_cols + remaining_cols\n",
    "\n",
    "        if len(all_feature_names) == len(feature_importances):\n",
    "            importance_df = pd.DataFrame({'Feature': all_feature_names, 'Importance': feature_importances})\n",
    "            importance_df = importance_df.sort_values(by='Importance', ascending=False)\n",
    "            print(\"Feature Importance:\\n\", importance_df)\n",
    "        else:\n",
    "            print(\"Mismatch between feature names and importance array lengths. Skipping feature importance.\")\n",
    "\n",
    "    # Step 3: Test alternative models\n",
    "    alternative_models = [\n",
    "        GradientBoostingClassifier(random_state=44),\n",
    "        AdaBoostClassifier(random_state=44),\n",
    "        LogisticRegression(max_iter=1000, class_weight='balanced', random_state=44)\n",
    "    ]\n",
    "\n",
    "    alternative_results = {}\n",
    "    for model in alternative_models:\n",
    "        alt_pipeline = Pipeline(steps=[\n",
    "            ('preprocessor', preprocessor),\n",
    "            ('classifier', model)\n",
    "        ])\n",
    "        alt_pipeline.fit(X_train_resampled, y_train_resampled)\n",
    "        y_pred_alt = alt_pipeline.predict(X_test)\n",
    "        alternative_results[model.__class__.__name__] = {\n",
    "            \"classification_report\": classification_report(y_test, y_pred_alt, output_dict=True),\n",
    "            \"confusion_matrix\": confusion_matrix(y_test, y_pred_alt).tolist()\n",
    "        }\n",
    "        print(f\"\\nModel: {model.__class__.__name__}\")\n",
    "        print(\"Classification Report:\\n\", classification_report(y_test, y_pred_alt))\n",
    "        print(\"Confusion Matrix:\\n\", confusion_matrix(y_test, y_pred_alt))\n",
    "\n",
    "    return {\n",
    "        \"optimized_model\": {\n",
    "            \"classification_report\": classification_report(y_test, y_pred, output_dict=True),\n",
    "            \"confusion_matrix\": confusion_matrix(y_test, y_pred).tolist(),\n",
    "            \"train_accuracy\": train_accuracy,\n",
    "            \"test_accuracy\": test_accuracy\n",
    "        },\n",
    "        \"alternative_models\": alternative_results\n",
    "    }\n",
    "\n",
    "# Call the function\n",
    "results = resample_and_evaluate(X_train, y_train, X_test, y_test, preprocessor, numerical_cols, remaining_cols)"
   ]
